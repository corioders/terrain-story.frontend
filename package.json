		"ip": "1.1.5",
		"md5": "2.3.0",
		"mini-css-extract-plugin": "2.3.0",
		"mock-require": "3.0.3",
		"node-sass-glob-importer": "5.3.2",
		"postcss-loader": "6.1.1",
