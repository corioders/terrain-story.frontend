{
	"name": "qr-games",
	"version": "0.0.1",
	"author": "corioders",
	"repository": "https://github.com/corioders/terrain-story.frontend",
	"browserslist": [
		"last 2 versions",
		">1%",
		"not dead",
		"not IE 11"
	],
	"scripts": {
    "clean": "rm -rf ./node_modules/.cache",
		"watch": "yarn run serve:dev:fast",
		"debug": "yarn run debug:dev",
		"produce": "yarn run build:prod",
		"serve:dev": "cross-env NODE_ENV=development webpack serve --no-stats --config ./config/webpack.dev.js",
		"serve:prod": "cross-env NODE_ENV=production webpack serve --no-stats --config ./config/webpack.prod.js",
		"serve:dev:fast": "cross-env FAST=true yarn run serve:dev",
		"serve:prod:fast": "cross-env FAST=true yarn run serve:prod",
		"debug:dev": "cross-env DEBUG=true yarn run serve:dev",
		"debug:prod": "cross-env DEBUG=true yarn run serve:prod",
		"analyze:dev": "cross-env ANALYZE=true yarn run build:dev",
		"analyze:prod": "cross-env ANALYZE=true yarn run build:prod",
		"build:dev": "cross-env NODE_ENV=development webpack --config ./config/webpack.dev.js",
		"build:prod": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js",
		"postinstall": "skip-postinstall || husky install"
	},
	"devDependencies": {
		"@babel/core": "7.15.8",
		"@babel/eslint-parser": "7.15.8",
		"@babel/plugin-transform-runtime": "7.15.8",
		"@babel/preset-env": "7.15.8",
		"@babel/runtime": "7.15.4",
		"@commitlint/cli": "13.2.1",
		"@soda/friendly-errors-webpack-plugin": "1.8.0",
		"@trivago/prettier-plugin-sort-imports": "3.0.0",
		"@types/google-protobuf": "3.15.5",
		"@types/webpack-env": "1.16.3",
		"@typescript-eslint/eslint-plugin": "5.2.0",
		"@typescript-eslint/parser": "5.2.0",
		"@vue/compiler-sfc": "3.2.20",
		"babel-loader": "8.2.3",
		"browser-sync": "2.27.7",
		"browser-sync-webpack-plugin": "2.3.0",
		"chalk": "4.1.2",
		"clean-webpack-plugin": "4.0.0",
		"commitlint-config-jira": "1.5.1",
		"commitlint-plugin-jira-rules": "1.5.1",
		"core-js": "3.19.0",
		"cross-env": "7.0.3",
		"css-loader": "6.5.0",
		"css-minimizer-webpack-plugin": "3.1.1",
		"dotenv": "10.0.0",
		"eslint": "8.1.0",
		"eslint-plugin-vue": "8.0.2",
		"eslint-webpack-plugin": "3.1.0",
		"html-webpack-plugin": "5.5.0",
		"husky": "7.0.4",
		"ip": "1.1.5",
		"mini-css-extract-plugin": "2.4.3",
		"mock-require": "3.0.3",
		"node-sass-glob-importer": "5.3.2",
		"postcss-loader": "6.2.0",
		"postcss-preset-env": "6.7.0",
		"prettier": "2.4.1",
		"sass": "1.43.4",
		"sass-loader": "12.3.0",
		"skip-postinstall": "1.0.0",
		"style-loader": "3.3.1",
		"terser-webpack-plugin": "5.2.4",
		"ts-loader": "9.2.6",
		"typescript": "4.4.4",
		"vue-loader": "16.5.0",
		"webpack": "5.61.0",
		"webpack-bundle-analyzer": "4.5.0",
		"webpack-cli": "4.9.1",
		"webpack-dev-server": "4.4.0",
		"webpackbar": "5.0.0-3"
	},
	"dependencies": {
		"@corioders/jskit": "0.0.6",
		"pinia": "2.0.0",
		"vfonts": "0.1.0",
		"naive-ui": "2.20.0",
		"vue": "3.2.12",
		"vue-router": "4.0.11"
	}
}
