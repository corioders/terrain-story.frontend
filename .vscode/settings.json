{
  "files.exclude": {
    "out/**/*.map": true,
  },
  "path-intellisense.mappings": {
    "@": "${workspaceRoot}/src",
  },
  "eslint.workingDirectories": [
    {
      "directory": "${workspaceRoot}",
      "changeProcessCWD": true
    }
  ],
  "prettier.configPath": ".prettierrc",
  "commitlint.config.file": "tools/config/commitlint.config.js",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "cSpell.words": [
    "Assemblyscript",
    "MINVER",
    "Proto",
    "Uncapitalize",
    "assemblyscript",
    "binaryen",
    "commitlint",
    "corejs",
    "etag",
    "grpc",
    "grpcweb",
    "lookpath",
    "nodelib",
    "pinia",
    "protoc",
    "toplevel",
    "unmount",
    "vuex",
    "wasm",
    "webassembly",
    "webpackbar"
  ]
}