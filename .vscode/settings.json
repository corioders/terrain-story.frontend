{
	"files.exclude": {
		"out/**/*.map": true,
	},
	"eslint.workingDirectories": [
		{
			"directory": "${workspaceRoot}",
			"changeProcessCWD": true
		}
	],
	"prettier.configPath": ".prettierrc",
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"cSpell.words": [
    "Assemblyscript",
    "MINVER",
    "Proto",
    "Uncapitalize",
    "assemblyscript",
    "binaryen",
    "commitlint",
    "corejs",
    "etag",
    "grpc",
    "grpcweb",
    "lookpath",
    "nodelib",
    "pinia",
    "protoc",
    "toplevel",
    "unmount",
    "vuex",
    "wasm",
    "webassembly",
    "webpackbar"
  ]
}
